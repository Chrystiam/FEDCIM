<tr id=<%= "user_#{user.id}" %>>

  <td><%= link_to user.id, user_path(user), :remote => true %></td>
  <td><%= user.nombre %></td>
  <td><%= user.telefono %></td>
  <td><%= user.username %></td>
  <td><%= user.email %></td>
  <td><%=l user.created_at %></td>
  <td>
    <%= link_to "editar", edit_user_path(user), :remote => true, :class => 'btn btn-mini' %>
   <%= link_to 'Eliminar', user,  :data => { :confirm => 'Esta Seguro?' }, :method => :delete, :remote => true, :class => 'btn btn-mini btn-danger' %>
  </td>
</tr>