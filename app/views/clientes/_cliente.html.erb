<tr id=<%= "cliente_#{cliente.id}" %>>
	
	<td><%= cliente.nombres %></td>
    <td><%= cliente.apellidos %></td>
    <td><%= cliente.cedula %></td>
    <td><%= cliente.empresa %></td>
    <td><%= cliente.nit %></td>
    <td><%= cliente.cargo %></td>
    <td><%= cliente.telefono %></td>
    <td><%= cliente.direccion %></td>
    <td><%= cliente.celular %></td>
    <td><%= cliente.fax %></td>
    <td><%= cliente.email %></td>
    <td><%= cliente.municipio.nombre %></td>

	<td>
        <div class="btn-group">
            <a class="btn btn-mini dropdown-toggle btn-success" data-toggle="dropdown" href="#"><span class="caret"></span>Action
            
            </a>
            <ul class="dropdown-menu anchmenu">
           
                <p><%= link_to 'Mostrar', cliente, :remote => true, :class => 'btn btn-mini btn-success' %></p>
                <p><%= link_to 'Editar', edit_cliente_path(cliente), :remote => true, :class => 'btn btn-mini' %></p>
                <p><%= link_to 'Eliminar', cliente,  :data => { :confirm => 'Esta Seguro?' }, :method => :delete, :remote => true, :class => 'btn btn-mini btn-danger' %></p>
            </ul>
      </div>
	</td>
</tr>

