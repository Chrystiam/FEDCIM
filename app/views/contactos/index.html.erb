<%- model_class = Contacto -%>
<div class="page-header">
  <h1>contactos</h1>
</div>
<table class="table table-striped  table-hover">
  <thead>
    <tr>
      <th>nombre</th>
      <th>apellido</th>
      <th>telefono></th>
      <th>celular</th>
      <th>email</th>
      <th>created_at</th>
    </tr>
  </thead>
  <tbody>
    <%= render @contactos%>
  </tbody>
</table>



<tfoot>
  <p><%= link_to "Nuevo contacto ", new_contacto_path, :remote => true, :class => 'btn btn-small btn-info' %></p>
</tfoot>


