<br><br>
<%- model_class = Contacto -%>

<div class="modal responsive">
	
	<div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-hidden="true" onclick="$.lazybox.close()">&times;</button>
        <h3>Contactos</h3>
   </div>
	
   	<div class="modal-body">
   		<div class="row">
			<div class="contactos">
				<div>
				  <p><%= @contacto.nombre %></p>
				  <p><%= @contacto.apellido %></p>
				  <p><%= @contacto.telefono %></p>
				  <p><%= @contacto.celular %></p>
				  <p><%= @contacto.email %></p>
				</div>  
			</div>
		</div>	
	</div>	
</div>
