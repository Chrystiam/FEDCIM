<tr id=<%= "contacto_#{contacto.id}" %>>

    <td><%= contacto.nombre %></td>
    <td><%= contacto.apellido %></td>
    <td><%= contacto.telefono %></td>
    <td><%= contacto.celular %></td>
    <td><%= contacto.email %></td>
    <td><%=l contacto.created_at %></td>
    <td>
	    <%= link_to 'Mostrar', contacto, :remote => true, :class => 'btn btn-mini' %>
	    <%= link_to 'Editar', edit_contacto_path(contacto), :remote => true,:class => 'btn btn-mini' %>
	    <%= link_to 'Eliminar', contacto, :data => { :confirm => 'Esta Seguro?' },:method => :delete, :remote => true, :class => 'btn btn-mini btn-danger' %>
    </td>
</tr> 